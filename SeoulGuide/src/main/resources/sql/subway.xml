<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
						"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mapper.subway">

	
	<select id="selectSubwayByName" resultType="com.seoul.guide.subway.DTO.SubwayDTO" parameterType="java.util.Map">
    <![CDATA[
        select * from subway where sub_name like concat('%', #{name}, '%')
    ]]>
	</select>
</mapper>						